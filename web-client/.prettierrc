{
  "printWidth": 140,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript",
        "singleQuote": true,
        "trailingComma": "es5",
        "arrowParens": "always"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "parser": "scss"
      }
    }
  ]
}